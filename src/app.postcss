/* Write your global styles here, in PostCSS syntax */
@tailwind base;
@tailwind components;
@tailwind utilities;

@layer base {
	@font-face {
		font-family: 'IBM Plex Sans';
		src: url('/Fonts/IBMPlexSans-Regular.ttf');
		font-weight: normal;
		font-display: swap;
	}
	@font-face {
		font-family: 'IBM Plex Sans';
		src: url('/Fonts/IBMPlexSans-SemiBold.ttf');
		font-weight: bold;
		font-display: swap;
	}
}

.btn{
  text-transform: none;
}
